<div class="slider-area {% if section.settings.section_width == 'full_width' %}container-fluid {% elsif section.settings.section_width == 'box_width' %}container-fluid section-padding-1{% elsif section.settings.section_width == 'default' %}default {% endif %} slider_style_1" id="section-{{ section.id }}" data-section="heroOwlSlider">
   
  <div class="slider-active-boxed owl-carousel dot-style-1 {{section.settings.dot_style}} nav-style-{% if section.settings.nav_style == 'style_1' %}1{% elsif section.settings.nav_style == 'style_3' %}3{% elsif section.settings.nav_style == 'style_4' %}2 nav_2_left{% else %}2{% endif %}" data-owl-carousel='{
 
      "loop": true,
      
      {% if section.settings.dot_style == 'dot_hide' %}
      	 "dots": false,
      {% else %}
     	 "dots": true,
      {% endif %}
      
      {% if section.settings.nav_style == 'style_5' %}
          "nav": false,
      {% else %}
          "nav": true,
      {% endif %}
      
      {% if settings.rtl_enable %}
      "rtl": true,
      {% endif %}
      "autoplay": false,
      {%- if section.settings.nav_style == 'style_1' -%}
      "navText": ["<i class=&#x27;sli sli-arrow-left&#x27;></i>","<i class=&#x27;sli sli-arrow-right&#x27;></i>"],
      {%- elsif section.settings.nav_style == 'style_2' -%}
      "navText": ["<i class=&#x27;sli sli-arrow-up&#x27;></i>","<i class=&#x27;sli sli-arrow-down&#x27;></i>"],
      {%- elsif section.settings.nav_style == 'style_4' -%}
      "navText": ["<i class=&#x27;sli sli-arrow-up&#x27;></i>","<i class=&#x27;sli sli-arrow-down&#x27;></i>"],
      {%- elsif section.settings.nav_style == 'style_3' -%}
      "navText": ["<i class=&#x27;sli sli-arrow-up&#x27;></i><span>Prev</span>","<i class=&#x27;sli sli-arrow-down&#x27;></i><span>Next</span>"],
	  {%- endif -%}
      "autoplayTimeout": 5000,
      "animateOut": "fadeOut",
      "animateIn": "fadeIn",
      "item": 1,
        "responsive": {
            "0": {
                "items": 1
            },
            "768": {
                "items": 1
            },
            "1000": {
                "items": 1
            }
      }}'>


      {%- for block in section.blocks -%}
      {%- case block.type -%}
      {%- when 'slide' -%}
      <div class="class-{{block.id}} single-slider d-flex align-items-center" style="background-color:{{ block.settings.sec_bg_color }};height: {% if section.settings.slider_height_opt == 'fixed_height' %}{{ section.settings.p_top }}px {% else %} 100vh {%endif%};{%- if block.settings.slider_bg_img -%}background-image: url('{{ block.settings.slider_bg_img | img_url: 'master' }}'){% endif %}">
        <div class="{% if section.settings.content_width == 'full_width' %}container-fluid {% elsif section.settings.content_width == 'box_width' %}container-fluid section-padding-1{% elsif section.settings.content_width == 'default' %}default container {% endif %}">
          <div class="row align-items-left {% if block.settings.content_position == 'right' %} flex-row-reverse {% elsif block.settings.content_position == 'center' %}justify-content-center {% elsif block.settings.content_position == 'left' %} justify-content-left{% endif %}">
  
            <div class="col-md-{{block.settings.slider_content_col}} col-sm-12 col-12">
              <div class="slider-content slider-animated-1 text-{% if block.settings.content_align == 'center' %}center{% elsif block.settings.content_align == 'right' %}right{% else %}left{% endif %}">
  
  
                {%- if block.settings.title_top != '' -%}
                <h3 class="animated">{{ block.settings.title_top }}</h3>
                {%- endif -%}
  
  
                {%- if block.settings.title1 != '' -%}
                <h1 class="animated">{{ block.settings.title1 }}</h1>
                {%- endif -%}
  
                {%- if block.settings.description != '' -%}
                <p class="animated">{{ block.settings.description }}</p>
                {%- endif -%}
  
                {%- if block.settings.btn != '' -%}
                <div class="slider-btn btn-hover">
                  <a class="animated" href="{{ block.settings.btn_url }}">
                    {{ block.settings.btn }} {% if block.settings.show_icon %} <i style="font-size:{{ block.settings.btn_font_size }}px;" class="sli sli-basket-loaded"></i>{% endif %}
                  </a>
                </div>
                {%- endif -%}
  
              </div>
            </div>
  
            {%- if block.settings.slider_img_enable -%}
            <div class="col-md-{{block.settings.slider_img_col}} col-sm-12 col-12">
              <div class="slider-single-img slider-animated-1">
                {%- if block.settings.slider_img -%}
                <img class="animated" src="{{ block.settings.slider_img | img_url: 'master' }}" alt="{{ image.alt }}">
                {%- else -%}
                <img class="animated" src="https://via.placeholder.com/620x877" alt="{{ image.alt }}">
                {%- endif -%}
              </div>
            </div>
            {%- endif -%}
  
          </div>
        </div>
  
  
  
 
  
        {%- if block.settings.slider_content_custom_style -%}    
        
            {%- assign header_font_top = block.settings.header_font_top -%}
            {%- assign header_font_slider = block.settings.header_font_slider -%}
            {%- assign paragraph_font_slider = block.settings.paragraph_font_slider -%}
            {%- assign button_font_slider = block.settings.button_font_slider -%} 

          <style>
            {{ header_font_top | font_face }}
            {{ header_font_slider | font_face }}
            {{ paragraph_font_slider | font_face }}
            {{ button_font_slider | font_face }}

            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content h3 {
              font-family: {{ header_font_top.family }}, {{ header_font_top.fallback_families }};
              font-style: {{ header_font_top.style }};
              font-weight: {{ header_font_top.weight }};
              color:{{ block.settings.header_top_color }};
              font-size:{{ block.settings.header_font_top_size }}px;
              line-height: {{block.settings.header_font_top_line_height}}px;
              margin-bottom: {{block.settings.header_font_top_margin_bottom}}px;
            }
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content h3 span {
              color: {{ settings.color_primary }};
            }

            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content h1 {
              font-family: {{ header_font_slider.family }}, {{ header_font_slider.fallback_families }};
              font-style: {{ header_font_slider.style }};
              font-weight: {{ header_font_slider.weight }};
              color:{{ block.settings.title1_color }};
              font-size:{{ block.settings.title1_font_size }}px;
              line-height: {{block.settings.heading_line_height}}px;
              margin-bottom: {{block.settings.title_margin_bottom}}px;
            }
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content h1 span {
              color: {{ settings.color_primary }};
            }
            
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content p {
              font-family: {{ paragraph_font_slider.family }}, {{ paragraph_font_slider.fallback_families }};
              font-style: {{ paragraph_font_slider.style }};
              font-weight: {{ paragraph_font_slider.weight }};
              color:{{ block.settings.description_color }};
              font-size:{{ block.settings.description_font_size }}px;
              line-height: {{block.settings.des_line_height}}px;
              margin:0 0 {{block.settings.des_margin_bottom}}px;
            }
            
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content p span {
              color: {{ settings.color_primary }};
            }
            
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content .slider-btn a {
              background-color: {{ block.settings.btn_bg_color }};
              color: {{ block.settings.btn_color }};
              border: 1px solid {{ block.settings.btn_border_color }};
              border-radius: {{ block.settings.btn_border_radius }}px;
              text-transform: unset;
              font-size:{{ block.settings.btn_font_size }}px;
              font-family: {{ button_font_slider.family }}, {{ button_font_slider.fallback_families }};
              font-style: {{ button_font_slider.style }};
              font-weight: {{ button_font_slider.weight }};
            }
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content .slider-btn{
              margin-top: {{block.settings.button_margin_top}}px;
            }
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content .slider-btn a i {
              color: {{ block.settings.btn_color }};
              font-size:{{ block.settings.btn_font_size }}px;
            }
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content .slider-btn a:hover {
              background-color: {{ block.settings.btn_hover_bg_color }};
              color: {{ block.settings.btn_hover_color }};
              border: 1px solid {{ block.settings.btn_hover_border_color }};
            }
            #section-{{ section.id }}.slider-area .class-{{block.id}}.single-slider .slider-content .slider-btn a:hover i {
              color: {{ block.settings.btn_hover_color }};
            }
            
            
            
            #section-{{ section.id }}.slider-area .owl-item.active .class-{{block.id}}.single-slider .slider-animated-1 h3 {
              -webkit-animation-name: {{block.settings.content_animation}};
              animation-name: {{block.settings.content_animation}};
            }
            #section-{{ section.id }}.slider-area .owl-item.active .class-{{block.id}}.single-slider .slider-animated-1 h1 {
              -webkit-animation-name: {{block.settings.content_animation}};
              animation-name: {{block.settings.content_animation}};
            }
            #section-{{ section.id }}.slider-area .owl-item.active .class-{{block.id}}.single-slider .slider-animated-1 p {
              -webkit-animation-name: {{block.settings.content_animation}};
              animation-name: {{block.settings.content_animation}};
            }
            #section-{{ section.id }}.slider-area .owl-item.active .class-{{block.id}}.single-slider .slider-animated-1 a {
              -webkit-animation-name: {{block.settings.content_animation}};
              animation-name: {{block.settings.content_animation}};
            }
            
            

          </style>
  
        {%endif%}
  
      </div>
  
      {%- endcase -%}
      {%- endfor -%}
  
  
    </div>
  </div>
  
  
  
  {%- if section.settings.nav_custom_style -%}  
  <style>
    #section-{{ section.id }}.slider-area .nav-style-1.owl-carousel .owl-nav div, 
    #section-{{ section.id }}.slider-area .nav-style-2 .owl-nav div, 
    #section-{{ section.id }}.slider-area .nav-style-3 .owl-nav div {
      color: {{ section.settings.controlnav_color }};
      background-color: {{ section.settings.controlnav_bg_color }};
      border: 1px solid {{ section.settings.controlnav_border_color }};
      border-radius:{{section.settings.nav_border_radius}}px;
    }
    #section-{{ section.id }}.slider-area .nav-style-1.owl-carousel .owl-nav div:hover, 
    #section-{{ section.id }}.slider-area .nav-style-2 .owl-nav div:hover, 
    #section-{{ section.id }}.slider-area .nav-style-3 .owl-nav div:hover {
      color: {{ section.settings.hover_controlnav_color }};
      background-color: {{ section.settings.hover_controlnav_bg_color }};
      border: 1px solid {{ section.settings.hover_controlnav_border_color }};
    }
  </style>
  {%endif%}
  
  
  {%- if section.settings.dot_custom_style -%}  
  <style>

    #section-{{ section.id }}.slider-area .dot-style-1 .owl-dots .owl-dot {
      border: 1px solid {{section.settings.dot_color}};
      outline: 1px solid {{section.settings.dot_color}};
    }

    #section-{{ section.id }}.slider-area .dot-style-1 .owl-dots .owl-dot.active {
      background-color: {{section.settings.dot_active_color}};
      border: 1px solid {{section.settings.dot_active_color}};
      outline: 1px solid {{section.settings.dot_active_color}};
    }    

  </style>
  {%endif%}
  
  
  
  
  

  
  
  {% schema %}
    {
